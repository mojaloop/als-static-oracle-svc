{
  "name": "@mojaloop/als-static-oracle-svc",
  "version": "0.0.29",
  "description": "An Account Lookup Service Oracle based on static configuration",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "_phase:build": "npm run build",
    "audit:fix": "npm audit fix",
    "audit:check": "npx audit-ci --config ./audit-ci.jsonc",
    "build": "npm run build:openapi; tsc -p ./tsconfig.build.json; tsc-alias -p ./tsconfig.build.json; cp -R config/*.json dist/config/; mkdir -p ./dist/src/interface; cp ./src/interface/*.yaml ./dist/src/interface/",
    "build:openapi": "redocly bundle --output ./src/interface/api.yaml --ext yaml ./src/interface/api-template.yaml",
    "dep:check": "npx ncu -e 2",
    "dep:update": "npx ncu -u",
    "docker:build": "docker build --build-arg NODE_VERSION=\"$(cat .nvmrc)-alpine\" -t als-static-oracle-svc:local -f ./Dockerfile ./",
    "docker:run": "docker run -p 3000:3000 als-static-oracle-svc:local",
    "docker:up": "docker-compose -f docker-compose.yml up",
    "docker:stop": "docker-compose -f docker-compose.yml stop",
    "docker:rm": "docker-compose -f docker-compose.yml rm -f -v",
    "docker:down": "docker-compose -f docker-compose.yml down -v",
    "docker:clean": "docker-compose -f docker-compose.yml down --rmi local",
    "lint": "eslint -c ./eslint.config.mjs ./src/**/*.ts ./test/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write --ignore-unknown \"src/**/*.ts\" \"test/**/*.ts\"",
    "pretest": "echo \"pretest - TODO...\"",
    "release": "standard-version --releaseCommitMessageFormat 'chore(release): {{currentTag}} [skip ci]'",
    "snapshot": "standard-version --no-verify --skip.changelog --prerelease snapshot --releaseCommitMessageFormat 'chore(snapshot): {{currentTag}}'",
    "standard": "echo '\\033[1;33m  This project uses eslint instead of standard. Use `npm run lint` instead.'",
    "start": "node ./dist/src/cli.js",
    "start:dev": "npm run build && ts-node src/cli.ts",
    "start:watch": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/cli.ts",
    "test": "npm run test:unit test:integration",
    "test:coverage": "jest --coverage --coverageThreshold='{}'",
    "test:coverage-check": "jest --config './jest.coverage.config.js' --coverage",
    "test:integration": "./test/scripts/run-integration-test.sh",
    "test:int": "jest --config './jest.integration.config.js' --runInBand --testMatch '**/test/integration/**/*.(test|spec).ts'",
    "test:junit": "jest --testMatch '**/test/unit/**/*.(test|spec).ts' --reporters=default --reporters=jest-junit",
    "test:unit": "jest --testMatch '**/test/unit/**/*.(test|spec).ts'",
    "test:xunit": "npm run test:unit",
    "test:functional": "echo 'No functional tests defined'",
    "watch": "tsc -w",
    "wait-4-docker": "node ./scripts/_wait4_all.js",
    "prepare": "husky || true"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "npm run lint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mojaloop/als-static-oracle-svc.git"
  },
  "keywords": [
    "oracle",
    "als",
    "static"
  ],
  "_moduleAliases": {
    "~": "dist/src"
  },
  "author": "Vijay Kumar <vijaya.guthi@infitx.com",
  "contributors": [
    "Vijay Kumar Guthi <vijaya.guthi@infitx.com>"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mojaloop/als-static-oracle-svc/issues"
  },
  "homepage": "https://github.com/mojaloop/als-static-oracle-svc#readme",
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@jest/globals": "^30.2.0",
    "@redocly/cli": "^2.12.7",
    "@types/convict": "^6.1.6",
    "@types/jest": "30.0.0",
    "@types/node": "^25.0.3",
    "@types/rc": "^1.2.4",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "add": "^2.0.6",
    "audit-ci": "^7.1.0",
    "axios": "1.13.2",
    "babel-jest": "^30.2.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-jest": "^29.5.0",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "jest-junit": "16.0.0",
    "lint-staged": "^16.2.7",
    "multi-file-swagger": "^2.3.0",
    "nodemon": "^3.1.11",
    "npm-check-updates": "19.2.0",
    "prettier": "3.7.4",
    "source-map-support": "0.5.21",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.4.6",
    "ts-node": "10.9.2",
    "tsc-alias": "1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@hapi/good": "^9.0.1",
    "@hapi/hapi": "^21.4.4",
    "@hapi/inert": "^7.1.0",
    "@hapi/vision": "^7.0.3",
    "@mojaloop/api-snippets": "^18.2.2",
    "@mojaloop/central-services-error-handling": "13.1.5",
    "@mojaloop/central-services-health": "15.2.1",
    "@mojaloop/central-services-metrics": "12.8.3",
    "@mojaloop/central-services-shared": "18.34.5",
    "@mojaloop/sdk-standard-components": "19.18.1",
    "ajv": "8.17.1",
    "ajv-keywords": "5.x.x",
    "blipp": "^4.0.2",
    "commander": "^14.0.2",
    "convict": "^6.2.4",
    "hapi-openapi": "^3.0.0",
    "hapi-swagger": "^17.3.2",
    "module-alias": "^2.2.3",
    "npm-run-all": "^4.1.5",
    "parse-strings-in-object": "^2.0.0",
    "rc": "^1.2.8"
  },
  "overrides": {
    "brace-expansion": "2.0.2",
    "cross-spawn": "7.0.6",
    "form-data": "4.0.4",
    "markdown-it": "12.3.2",
    "oas-validator": {
      "ajv": "^6.12.6"
    },
    "shins": {
      "ajv": "8.x.x",
      "ejs": "3.1.10",
      "sanitize-html": "2.12.1",
      "markdown-it": "12.3.2"
    },
    "yargs": {
      "yargs-parser": "21.1.1"
    },
    "jsonpointer": "5.0.0"
  }
}
